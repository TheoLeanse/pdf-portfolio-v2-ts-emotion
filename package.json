    "gatsby-cli": "^2.7.47",
    "gatsby-image": "^2.0.31",
    "gatsby-plugin-canonical-urls": "^2.0.10",
    "gatsby-plugin-emotion": "^4.0.4",
    "gatsby-plugin-netlify": "^2.0.11",
    "gatsby-plugin-netlify-cms": "^3.0.12",
